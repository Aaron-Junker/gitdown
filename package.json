{
  "name": "gitdown",
  "description": "Github markdown preprocessor.",
  "keywords": [
    "github",
    "markdown",
    "table of contents",
    "toc",
    "include",
    "variable",
    "transclusion"
  ],
  "version": "2.4.4",
  "main": "./dist/index.js",
  "bin": "./dist/bin/index.js",
  "author": {
    "name": "Gajus Kuizinas",
    "email": "gajus@gajus.com",
    "url": "http://gajus.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/gitdown"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "lint": "pragmatist lint",
    "test": "pragmatist --type-assertions test",
    "build": "pragmatist --es5 build",
    "watch-lint": "pragmatist watch-lint",
    "watch-test": "pragmatist --type-assertions watch-test",
    "watch-build": "pragmatist --es5 watch-build"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "nock": "^7.4.0",
    "pragmatist": "^3.0.21",
    "require-new": "^1.1.0",
    "sinon": "^2.0.0-pre"
  },
  "dependencies": {
    "bluebird": "^3.3.4",
    "deadlink": "^1.1.3",
    "filesize": "^3.2.1",
    "gitinfo": "^1.2.2",
    "glob": "^7.0.3",
    "jsonfile": "^2.2.3",
    "lodash": "^4.6.1",
    "markdown-contents": "^1.0.8",
    "marked": "^0.3.5",
    "moment": "^2.12.0",
    "stack-trace": "^0.0.9",
    "url-extractor": "^2.0.0",
    "yargs": "^4.3.2"
  }
}
